{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "clipboard-manager:default",
    "notification:default",
    "stronghold:default",
    "os:default",
    "os:allow-os-type",
    "os:allow-locale",
    "os:allow-hostname",
    "store:allow-clear",
    "store:allow-get-store",
    "store:allow-has",
    "store:allow-delete",
    "store:allow-set",
    "store:default",
    "dialog:default"
  ]
}
